    "import numpy as np\n",
    "\n",
    "custom_map3x3 = [\n",
    "    'SFF',\n",
    "    'FFF',\n",
    "    'FMG',\n",
    "]\n",
    "\n",
    "env = gym.make(\"FrozenLake-v1\", desc=custom_map3x3)\n",
    "\n",
    "n_states = env.observation_space.n \n",
    "n_actions = env.action_space.n \n",
    "\n",
    "r = np.zeros(n_states)\n",
    "r[-1] = 1\n",
    "\n",
    "gamma = 0.8\n",
    "\n"
   ]
